#!/bin/bash# first test of an actor# actor1 -v -n adam -f adam.out -h 192.168.2.1 -p 9900 << CONFIGactor1 -d0 -n adam -h 192.168.2.1 -p 9900 << CONFIG192.168.2.13 9800 midi   note_on   midi_channel 9    low   0x20    hi    0x70    low_velocity   0x10    hi_velocity    0x70    end# this is a commentshell echo here 1if %var1 -eq 1   {   shell echo here 2   if %var2 -eq 2      192.168.2.1 9700 : nop 00   }if %mnote -eq 42   shell echo note 2aset var2 0CONFIG